<%
	min_li = 2 #最少的li數量
	max_li = 10 #最多的li數量

	li_length = (min_li..max_li).to_a.sample
	this = rand( li_length )
%>

<div class="list-multiple page-click <% if class_set %><%= class_set %><% end %>" data-index="<%= index %>" data-type="<%= $module %>" <% if func_set %> data-func="<%= func_set %>"<% end %> ><div class="in">
	<% if header_text %>
		<div class="hd"><div class="in">
			<h4><span><a href="#"><%= header_text %></a></span></h4>
		</div></div>
	<% end %>
	<div class="ct"><div class="in"><ul data-index="1" data-child="2"><li data-index="1" class="first"><span><a href="#">最前頁</a></span></li><li data-index="2" class="prev"><span><a href="#">上一頁</a></span></li></ul><ul data-index="2" data-child="2"><li data-index="1" class="next"><span><a href="#">下一頁</a></span></li><li data-index="2" class="last"><span><a href="#">最後頁</a></span></li></ul><ul data-index="3" data-child="<%= li_length %>"><% li_length.times do |i| %><li data-index="<%= i + 1 %>" <%if ( i == this ) %> class="is-active" <% end %> ><span><a href="#"><%= i + 1 %></a></span></li><% end %></ul></div></div>
</div></div>